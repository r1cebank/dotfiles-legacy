{
  "root":      true,
  "extends":  "airbnb/base",

  "rules": {
    "new-cap": 0,
    "no-undef": 2,
    "func-names": 0,
    "no-console": 1,
    "key-spacing": 0,
    "padded-blocks": 0,
    "no-const-assign": 2,
    "indent": 2, 4,
    "no-param-reassign": 0,
    "indent": 2,

    "comma-dangle": [1, "never"]
  }
}
